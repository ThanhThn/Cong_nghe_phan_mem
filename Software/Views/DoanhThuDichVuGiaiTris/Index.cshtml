@model IEnumerable<Software.Models.DoanhThuDichVuGiaiTri>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Ngay)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SoTienThu)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Ngay)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SoTienThu)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MaDoanhThu">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MaDoanhThu">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MaDoanhThu">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
